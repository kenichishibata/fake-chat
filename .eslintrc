{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules":{
    "array-bracket-spacing":0,
    "arrow-body-style":0,
    "react/jsx-curly-spacing": 0,
    "react/prefer-stateless-function": 0,
    "max-len": 0,
    "func-names": 0,
    "no-undef": 0,
    "no-console": 0,
    "react/no-string-refs": 0, 
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },

  "env": {
    "browser": true,
    "node": true,
    "es6": true,
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
    }
  }